{
  "mltrainer_data_access_configuration": {
    "version": "1.0",
    "last_updated": "2025-07-04T08:15:00Z",
    "description": "Technical data access infrastructure for mlTrainer",
    
    "api_access_status": {
      "polygon_api": {
        "status": "active",
        "key_available": true,
        "data_types": ["stocks", "options", "forex", "crypto"],
        "delay": "15_minutes",
        "rate_limits": "5_calls_per_minute_free_tier",
        "base_url": "https://api.polygon.io"
      },
      "fred_api": {
        "status": "active", 
        "key_available": true,
        "data_types": ["economic_indicators", "interest_rates", "gdp", "inflation"],
        "delay": "real_time_economic_data",
        "rate_limits": "120_calls_per_minute",
        "base_url": "https://api.stlouisfed.org/fred"
      }
    },
    
    "data_pipeline_configuration": {
      "retrieval_method": "api_calls_through_technical_facilitator",
      "data_delay": "15_minutes_for_market_data",
      "refresh_frequency": "configurable_by_mltrainer",
      "data_formats": ["json", "pandas_dataframe"],
      "error_handling": "compliance_enforced_responses"
    },
    
    "storage_system": {
      "primary_storage": "file_system_json",
      "historical_data_path": "data/historical/",
      "results_storage_path": "data/results/",
      "session_data_path": "data/sessions/",
      "backup_system": "automated_daily_backup",
      "retention_policy": "30_days_rolling_cleanup"
    },
    
    "technical_facilitator_endpoints": {
      "data_retrieval": "/api/facilitator/data-sources",
      "historical_access": "/api/facilitator/load-results/historical_data",
      "real_time_status": "/api/facilitator/system-status",
      "save_analysis": "/api/facilitator/save-results",
      "model_execution": "/api/facilitator/execute-model"
    },
    
    "mltrainer_access_methods": {
      "polygon_market_data": {
        "endpoint": "backend.data_sources.DataSourceManager.get_polygon_data()",
        "parameters": ["symbol", "timeframe", "start_date", "end_date"],
        "example": "AAPL, 15min, 2025-01-01, 2025-07-04"
      },
      "fred_economic_data": {
        "endpoint": "backend.data_sources.DataSourceManager.get_fred_data()",
        "parameters": ["series_id", "start_date", "end_date"],
        "example": "GDP, 2024-01-01, 2025-07-04"
      },
      "historical_storage": {
        "save_method": "technical_facilitator.save_results()",
        "load_method": "technical_facilitator.load_results()",
        "formats": ["json", "pickle", "csv"]
      }
    },
    
    "data_compliance": {
      "verification_required": true,
      "synthetic_data_forbidden": true,
      "authorized_sources_only": ["polygon", "fred"],
      "unknown_data_response": "I don't know. But based on the data, I would suggest..."
    }
  },
  
  "system_readiness": {
    "api_keys_verified": true,
    "data_sources_operational": true,
    "storage_system_ready": true,
    "technical_facilitator_active": true,
    "mltrainer_access_granted": true
  }
}